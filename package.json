{
  "name": "fritz-xml-2vcard",
  "version": "1.2.0",
  "description": "Convert FRITZ!Box address book XML format to vcards.",
  "main": "lib-convert.js",
  "bin": "./convert.js",
  "dependencies": {
    "pixl-xml": "^1.0.13",
    "vcards-js": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.2",
    "clipboard": "^2.0.1",
    "file-saver": "^1.3.8",
    "jasmine": "^3.2.0",
    "jquery": "^3.3.1",
    "jquery-modal": "^0.9.1",
    "jszip": "^3.1.5",
    "node-git-version": "^0.1.2",
    "uglify-js": "^3.4.7"
  },
  "scripts": {
    "test": "jasmine",
    "version": "cd src && ../node_modules/.bin/node-git-version",
    "escompat": "mkdir -p lib && ./node_modules/.bin/babel src --out-dir lib",
    "bundle": "browserify lib/* -o dist/fritzXML2vcard-browser.js",
    "minify": "uglifyjs dist/fritzXML2vcard-browser.js -o dist/fritzXML2vcard-browser-min.js",
    "cleanup": "rm src/version.js lib/version.js",
    "build": "npm run version && npm run escompat && npm run bundle && npm run minify && npm run cleanup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rillke/fritzXML2vcard.git"
  },
  "keywords": [
    "xml-converter",
    "address-book",
    "fritzbox",
    "vcard"
  ],
  "author": "Rainer Rillke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rillke/fritzXML2vcard/issues"
  },
  "homepage": "https://github.com/Rillke/fritzXML2vcard#readme",
  "babel": {
    "presets": [
      "env"
    ]
  }
}
